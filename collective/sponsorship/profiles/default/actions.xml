<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="user" meta_type="CMF Action Category">
  <object name="add_sponsor" meta_type="CMF Action"
     i18n:domain="plone">
   <property name="title" i18n:translate="">Add Sponsors</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${portal_url}/sponsorships/++add++collective.sponsorship.sponsor</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Manage Site"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="manage_sponsorships" meta_type="CMF Action"
     i18n:domain="plone">
   <property name="title" i18n:translate="">Manage Sponsorships</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${portal_url}/@@manage-sponsorships</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Manage Site"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
